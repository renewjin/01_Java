package com.kh.goodShop;

import java.util.*;

public class 컴퓨터가게 {
	public static void main(String[] args) {
		// 모든 제품 HashMap
		Map<String, Integer> 제품 = new HashMap<>();
		// 상품추가 put
		// 노트북 700000
		// 데스크탑 20000
		// 모니터 5000
		// 마우스 3000
		// 키보드 2000
		
		제품.put("노트북", 700000);
		제품.put("데스크탑", 20000);
		제품.put("모니터", 5000);
		제품.put("마우스", 3000);
		제품.put("키보드", 2000);
		System.out.println(제품);
		
		System.out.println("컴퓨터 상품 목록");
		// Entry 사용해서 출력
		for (Map.Entry<String, Integer> e : 제품.entrySet()) {
			String 제품명 = e.getKey();
			int 가격 = e.getValue();
			System.out.println(제품명 + " : " + 가격);
		}
	}
}
